@using System.Reflection;
@using System.Reflection.Emit;
@using System.ComponentModel.DataAnnotations;
@typeparam TModel
@inherits FieldStringBase<TModel>
 <Row Gutter="16">
        <AntDesign.Col Class="gutter-row" Span="6">
            @FieldLabel
        </AntDesign.Col>
          <AntDesign.Col Class="gutter-row" Span="12">
                <AntDesign.Input TValue="string" Value="@FieldValue" ValueChanged="async(e)=>await changeValue(e)" >
                </AntDesign.Input>
        </AntDesign.Col>

 </Row>
  

